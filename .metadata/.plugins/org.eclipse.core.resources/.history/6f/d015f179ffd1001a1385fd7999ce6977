
public class TestaCondicional {
	public static void main(String[] args) {
		System.out.println("-- testando condicionais -- \n");
		int idade = 20;
		if (idade >= 18) {
			System.out.println("você tem mais de 18 anos"); System.out.println("Seja Bem-vindo");
		} else {
			System.out.println("você não tem mais de 18 anos");
		}
	}

}
